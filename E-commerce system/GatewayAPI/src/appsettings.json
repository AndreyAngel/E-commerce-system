{
  "Kestrel": {

    "Limits": {
      "MaxConcurrentConnections": 100,

      "MaxRequestBodySizeInBytes": 10240,

      "MinRequestBodyDataRate": {
        "BytesPerSecond": 100,
        "GracePeriodInSeconds": 10
      },

      "MinResponseDataRate": {
        "BytesPerSecond": 100,
        "GracePeriodInSeconds": 10
      }
    },

    "Endpoints": {
      "Https": {
        "Url": "https://localhost:44389"
      }
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
